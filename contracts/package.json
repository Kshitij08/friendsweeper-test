{
  "name": "friendsweeper-nft-contracts",
  "version": "1.0.0",
  "description": "Smart contracts for Friendsweeper NFT minting",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy:base-sepolia": "hardhat run scripts/deploy-nft-contract.js --network baseSepolia",
    "deploy:base": "hardhat run scripts/deploy-nft-contract.js --network base",
    "deploy:local": "hardhat run scripts/deploy-nft-contract.js --network localhost",
