    "verify:base": "hardhat verify --network base"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "hardhat": "^2.19.0",
    "dotenv": "^16.3.0"
  },
  "keywords": [
    "nft",
    "ethereum",
    "base",
    "friendsweeper",
    "smart-contracts"
  ],
  "author": "Friendsweeper Team",
  "license": "MIT"
}
